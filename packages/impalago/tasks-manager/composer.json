{
    "name": "impalago/tasks-manager",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "impalago",
        "tasks-manager"
    ],
    "homepage": "https://github.com/impalago/tasks-manager",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Ivan Kolesnikov",
            "email": "impalago@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "yajra/laravel-datatables-oracle": "^6.15",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Impalago\\TasksManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Impalago\\TasksManager\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
