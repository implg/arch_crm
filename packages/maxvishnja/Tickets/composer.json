{
    "name": "maxvishnja/Tickets",
    "type": "library",
    "description": "A system of communication between users through ticketing",
    "keywords": [
        "maxvishnja",
        "Tickets"
    ],
    "homepage": "https://github.com/maxvishnja/Tickets",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Vishnevsky",
            "email": "w.m.a@rambler.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "yajra/laravel-datatables-oracle": "~6.0",
        "jenssegers/date": "^3.0",
        "mews/purifier": "^2.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "maxvishnja\\Tickets\\": "src/",
            "maxvishnja\\Tickets\\Http\\Controllers\\": "src/Http/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "maxvishnja\\Tickets\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "php artisan vendor:publish",
        ],
        "format": "phpcbf --standard=psr2 src/"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
